<Window x:Class="materialApp.Print_Details"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:materialApp"
        mc:Ignorable="d"
        Title="Print_Details" Height="800" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="10"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="10"/>
        </Grid.ColumnDefinitions>
        <WebBrowser Grid.Column="1" Grid.Row="1" x:Name="pdfShow"/>
        <DockPanel Grid.Column="1" Grid.Row="2" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,0,0">
            <Button Click="Ok_print" Width="70" Height="40">
                Tlacit
            </Button>
            <Button Click="Back" Width="70" Height="40" Margin="30,0,0,0" >
                Zrus
            </Button>
        </DockPanel>
    </Grid>
</Window>
